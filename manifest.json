{
    "name": "iToken",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "抓取线上Token并同步到本地",
    "permissions": ["cookies", "storage", "scripting", "tabs", "declarativeNetRequest", "declarativeNetRequestWithHostAccess", "webRequest", "webRequestBlocking"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/logo.png"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": ["demo.json"],
            "matches": ["<all_urls>"]
        }
    ]
}
